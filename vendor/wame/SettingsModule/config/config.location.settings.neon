services:
	- Wame\LocationModule\Vendor\Wame\SettingsModule\Components\IContinentListControlFactory
	- Wame\LocationModule\Vendor\Wame\SettingsModule\Components\IStateListControlFactory


	StateListForm:
		class: Wame\LocationModule\Vendor\Wame\SettingsModule\Forms\StateListForm


	ContinentSettings:
		class: Wame\LocationModule\Vendor\Wame\SettingsModule\Registers\Continent


	StateSettings:
		class: Wame\LocationModule\Vendor\Wame\SettingsModule\Registers\State


	SettingsGroupRegister:
		setup:
			- add(@ContinentSettings, 'Continent')
			- add(@StateSettings, 'State')
